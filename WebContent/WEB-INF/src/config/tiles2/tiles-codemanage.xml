<tiles-definitions>

	<!-- 代碼管理 Start -->
 	<definition name="code.manage.layout" extends="default.layout" >
    	<put-attribute name="head" value="code.manage.head" />
    	<put-attribute name="menu" value="code.manage.menu" />
    </definition>
	
	<definition name="code.manage.head" extends="default.head" >
    	<put-attribute name="css" value="/ftl/codeManage/css.ftl" />
    </definition>
	
	<definition name="code.manage.menu" extends="default.menu" >
    	<put-attribute name="submenu" value="/ftl/codeManage/submenu.ftl" />
    </definition>
    
    
	<!-- 再行銷追蹤view  -->
	<definition name="retargeting.tracking.view" extends="code.manage.layout">
	 	<put-attribute name="head" value="retargeting.tracking.head" />
        <put-attribute name="body" value="/ftl/codeManage/retargeting/retargeting.tracking.view.ftl" />
    </definition>
    
    <definition name="retargeting.tracking.head" extends="default.head" >
    	<put-attribute name="css" value="/ftl/codeManage/css.ftl" />
    	<put-attribute name="js" value="/ftl/codeManage/retargeting/retargeting.tracking.js.ftl" />
    </definition>
    
   	<!-- 新增再行銷追蹤view  -->
   	<definition name="add.retargeting.tracking.view" extends="code.manage.layout">
	 	<put-attribute name="head" value="add.retargeting.tracking.head" />
        <put-attribute name="body" value="/ftl/codeManage/retargeting/add.retargeting.tracking.view.ftl" />
    </definition>
    
    <definition name="add.retargeting.tracking.head" extends="default.head" >
    	<put-attribute name="css" value="/ftl/codeManage/css.ftl" />
    	<put-attribute name="js" value="/ftl/codeManage/retargeting/add.retargeting.tracking.js.ftl" />
    </definition>
    
    
    
    
    
	
